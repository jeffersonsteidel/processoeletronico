	<parameter name="COD_SERV" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT
     servidor.`serv_siape` AS servidor_serv_siape,
     servidor.`serv_nome` AS servidor_serv_nome,
     servidor.`serv_sexo` AS servidor_serv_sexo,
     servidor.`serv_email` AS servidor_serv_email,
     servidor.`serv_data_nasc` AS servidor_serv_data_nasc,
     servidor.`serv_nome_pai` AS servidor_serv_nome_pai,
     servidor.`serv_nome_mae` AS servidor_serv_nome_mae,
     corpele.`cor_pele_desc` AS corpele_cor_pele_desc,
     estadocivil.`est_civ_desc` AS estadocivil_est_civ_desc,
     cidade.`cid_desc` AS cidade_cid_desc,
     pais.`pais_desc` AS pais_pais_desc,
     gruposanguineo.`grupo_sang_desc` AS gruposanguineo_grupo_sang_desc,
     estado.`est_uf` AS estado_est_uf
FROM
     `corpele` corpele INNER JOIN `servidor` servidor ON corpele.`cor_pele_cod` = servidor.`cor_pele_cod`
     INNER JOIN `estadocivil` estadocivil ON servidor.`est_civ_cod` = estadocivil.`est_civ_cod`
     INNER JOIN `cidade` cidade ON servidor.`cid_cod_nasc_serv` = cidade.`cid_cod`
     LEFT JOIN  pais ON servidor.`pais_cod` = pais.`pais_cod`
     INNER JOIN `gruposanguineo` gruposanguineo ON servidor.`grupo_sang_cod` = gruposanguineo.`grupo_sang_cod`
     INNER JOIN `estado` estado ON cidade.`est_cod` = estado.`est_cod`
WHERE
     servidor.`serv_cod` = $P{COD_SERV}]]></queryString>

