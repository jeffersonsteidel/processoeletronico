<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.progepe.entity.Autenticacao" table="AUTENTICACAO">
		<id name="siape" column="AUT_SIAPE">
		</id>
		<property name="senha" column="AUT_SENHA" />	
		<property name="indAdministrador" column="AUT_IND_ADMINISTRADOR" type="boolean"/>	
		<property name="indDiretor" column="AUT_IND_DIRETOR" type="boolean"/>	
		<property name="indSecretaria" column="AUT_IND_SECRETARIA" type="boolean"/>	
	</class>
</hibernate-mapping>