<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.progepe.entity.AreaConhecimento" table="AREACONHECIMENTO">
		<id name="codigo" column="AREA_CONH_COD">
			<generator class="increment"></generator>  
		</id>
		<property name="descricao" column="AREA_CONH_DESC" />			
	</class>
</hibernate-mapping>