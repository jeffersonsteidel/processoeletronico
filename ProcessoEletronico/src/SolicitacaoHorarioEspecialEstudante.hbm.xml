<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<joined-subclass name="br.com.progepe.entity.SolicitacaoHorarioEspecialEstudante"
		extends="br.com.progepe.entity.Solicitacao">
		<key column="SOLIC_COD"></key>
		<property name="curso" column="SOLIC_HOR_EPS_EST_CURSO" />
		<property name="instituicao" column="SOLIC_HOR_EPS_EST_INST" />
		<property name="motivo" column="SOLIC_HOR_EPS_EST_MOTIVO" />
	</joined-subclass>
</hibernate-mapping>