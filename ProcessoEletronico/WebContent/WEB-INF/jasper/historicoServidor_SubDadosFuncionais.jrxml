	<queryString><![CDATA[SELECT
     servidor.`serv_data_adm_cargo` AS servidor_serv_data_adm_cargo,
     servidor.`serv_data_adm_serv_pub` AS servidor_serv_data_adm_serv_pub,
     situacaofuncional.`sit_fun_desc` AS situacaofuncional_sit_fun_desc,
     padrao.`pad_nivel` AS padrao_pad_nivel,
     cargo.`carg_desc` AS cargo_carg_desc,
     lotacao.`lot_desc` AS lotacao_lot_desc,
     servidor.`serv_id_unica` AS servidor_serv_id_unica,
     servidor.`serv_data_saida` AS servidor_serv_data_saida,
     regimetrabalho.`reg_trab_desc` AS regimetrabalho_reg_trab_desc,
     classe.`clas_sigla` AS classe_clas_sigla,
     servidor.`serv_ramal` AS servidor_serv_ramal,
     servidor.`serv_siape` AS servidor_serv_siape
FROM
     `situacaofuncional` situacaofuncional INNER JOIN `servidor` servidor ON situacaofuncional.`sit_fun_cod` = servidor.`sit_fun_cod`
     INNER JOIN `padrao` padrao ON servidor.`pad_cod` = padrao.`pad_cod`
     INNER JOIN `cargo` cargo ON servidor.`carg_cod` = cargo.`carg_cod`
     INNER JOIN `lotacao` lotacao ON servidor.`lot_cod` = lotacao.`lot_cod`
     INNER JOIN `regimetrabalho` regimetrabalho ON servidor.`reg_trab_cod` = regimetrabalho.`reg_trab_cod`
     INNER JOIN `classe` classe ON cargo.`clas_cod` = classe.`clas_cod`
WHERE
     servidor.`serv_cod` = $P{COD_SERV}]]></queryString>

