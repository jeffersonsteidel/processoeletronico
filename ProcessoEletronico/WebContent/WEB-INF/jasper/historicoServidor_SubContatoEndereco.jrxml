	<queryString><![CDATA[SELECT
     endereco.`end_rua` AS endereco_end_rua,
     endereco.`end_bairro` AS endereco_end_bairro,
     endereco.`end_cep` AS endereco_end_cep,
     endereco.`end_num` AS endereco_end_num,
     endereco.`end_comp` AS endereco_end_comp,
     cidade.`cid_desc` AS cidade_cid_desc,
     estado.`est_desc` AS estado_est_desc,
     servidor.`serv_tel_cel` AS servidor_serv_tel_cel,
     servidor.`serv_tel_fixo` AS servidor_serv_tel_fixo,
     servidor.`serv_email` AS servidor_serv_email
FROM
     `cidade` cidade INNER JOIN `endereco` endereco ON cidade.`cid_cod` = endereco.`cid_cod`
     INNER JOIN `estado` estado ON cidade.`est_cod` = estado.`est_cod`
     INNER JOIN `servidor` servidor ON cidade.`cid_cod` = servidor.`cid_cod_nasc_serv`
     AND endereco.`end_cod` = servidor.`end_cod`
WHERE servidor.`serv_cod` = $P{COD_SERV}]]></queryString>

